    "handlebars": "4.7.7",
    "nanoid": "4.0.0",
    "parcel": "2.8.2",
    "parcel-transformer-hbs": "1.0.4",
    "rimraf": "3.0.2",
    "stylelint": "14.16.0",
    "stylelint-config-standard-scss": "6.1.0",
