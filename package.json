{
  "name": "middle.messenger.praktikum.yandex",
  "version": "1.0.0",
  "repository": "git@github.com:ErikKhasanov/middle.messenger.praktikum.yandex.git",
  "author": "erik <erikkhasanov@yandex.ru>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf ./dist",
    "start": "yarn build && node ./server.js",
    "dev": "parcel ./src/index.html --no-source-maps --no-cache",
    "build": "yarn clean && parcel build ./src/index.hbs",
    "lint": "yarn eslint --fix && yarn stylelint --fix"
  },
  "alias": {
    "core": "./src/core",
    "components": "./src/components",
    "pages": "./src/pages",
    "helpers": "./src/helpers"
  },
  "dependencies": {
    "express": "4.18.2"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "2.7.0",
    "eslint": "^8.30.0",
    "eslint-config-airbnb": "^19.0.4",
    "handlebars": "4.7.7",
    "nanoid": "^4.0.0",
    "parcel": "2.7.0",
    "parcel-transformer-hbs": "1.0.4",
    "rimraf": "3.0.2",
    "stylelint": "^14.16.0",
    "typescript": "^4.9.4"
  }
}
