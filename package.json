{
  "name": "middle.messenger.praktikum.yandex",
  "version": "1.0.0",
  "repository": "git@github.com:ErikKhasanov/middle.messenger.praktikum.yandex.git",
  "author": "erik <erikkhasanov@yandex.ru>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf ./dist",
    "start": "npm run build && node ./server.js",
    "dev": "parcel ./src/index.html --no-cache",
    "build": "npm run clean && parcel build ./src/index.html",
    "lint": "npx eslint --fix --ext .ts,.js, src/ && npx stylelint --allow-empty-input **/*.css"
  },
  "alias": {
    "core": "./src/core",
    "components": "./src/components",
    "pages": "./src/pages",
    "helpers": "./src/helpers",
    "styles": "./src/styles"
  },
  "dependencies": {
    "express": "4.18.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.47.1",
    "@typescript-eslint/parser": "5.47.1",
    "babel-eslint": "10.1.0",
    "eslint": "^8.32.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "handlebars": "4.7.7",
    "nanoid": "4.0.0",
    "parcel": "2.8.2",
    "parcel-transformer-hbs": "1.0.4",
    "prettier": "2.8.3",
    "rimraf": "3.0.2",
    "stylelint": "14.16.0",
    "stylelint-config-standard": "29.0.0",
    "typescript": "4.9.4",
    "typescript-eslint": "0.0.1-alpha.0"
  }
}
